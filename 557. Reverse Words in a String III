class Solution {
public:
    
    
    string reverseWords(string s) {
    int start=0,end=0;
        for(end=0;end!=s.size();end++)
        {
            if(isspace(s[end]))
            {
            reverse(s.begin()+start,s.begin()+end);
                start=end+1;
            }
            else if(end==s.size()-1)
            {
                reverse(s.begin()+start,s.begin()+end+1);
            }
        }
        return s;
    }
};
